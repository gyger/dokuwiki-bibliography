{
  "name": "gyger/bibliography",
  "description": "Bibliography is a plugin for dokuwiki",
  "homepage": "https://samuel.gyger.at",
  "type": "project",
  "license": "MIT",
  "require": {
      "php": ">=5.6",
      "citation-style-language/locales":"@dev",
      "citation-style-language/styles-distribution":"@dev",
      "seboettg/citeproc-php": "^2"
    },
  "config": {
    "platform": {
        "php": "5.6"
    }
  },
  "repositories": [
    {
        "type": "package",
        "package": {
            "name": "citation-style-language/locales",
            "version":"1.0.0",
            "source": {
                "type": "git",
                "url": "https://github.com/citation-style-language/locales.git",
                "reference": "master"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "name": "citation-style-language/styles-distribution",
            "version":"1.0.0",
            "source": {
                "type": "git",
                "url": "https://github.com/citation-style-language/styles-distribution.git",
                "reference": "master"
            }
        }
    }
  ],
  "prefer-stable": true
}